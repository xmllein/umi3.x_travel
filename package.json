{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "start:no-mock": "MOCK=none umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "dayjs": "^1.11.7",
    "project-libs": "^1.1.8",
    "prop-types": "^15.8.1",
    "rc-form": "^2.4.12",
    "react": "17.x",
    "react-awesome-swiper": "^1.4.16",
    "react-dom": "17.x",
    "react-icons": "^4.7.1",
    "think-react-store": "^1.6.71",
    "umi": "^3.5.36"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.36",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
